#include <iostream>
using namespace std;
int main(){
    char array[1000];
    cout<<"Enter Strings : ";
    cin>>array;

    int num[128]={0};
    int i;
    for( i=0; array[i]!='\0'; i++){
        num[array[i]]++;

    }for(int j=0; j<128; j++){
        if(num[j]>0){
            cout<<array[1000]<<" is prasent "<<num[j]<<endl;
        }
    }
    
    return 0;
}
